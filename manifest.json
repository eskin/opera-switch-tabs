{
    "manifest_version": 2,
    "name": "Switch Tabs by hotkeys",
    "description": "Позволяет переключаться между вкладками по хоткеям. Используйте меню хоткеев расширений для установки нужного клавиатурного сокращения",
    "version": "1.0.0",
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "permissions": ["tabs"],
    "background": {
        "scripts": ["background.js"]
    },
    "commands": {
        "previoustab": {
            "suggested_key": {
                "default": "Ctrl+Left"
            },
            "description": "Предыдущая вкладка"
            },
        "nexttab": {
            "suggested_key": {
                "default": "Ctrl+Right"
            },
            "description": "Следующая вкладка"
        }
    }
}
