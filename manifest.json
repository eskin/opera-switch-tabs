{
  "manifest_version": 2,

  "name": "Switch tabs with Ctrl+PgUp/PgDn",
  "description": "Lets you switch tabs using Ctrl-PageUp and Ctrl-PageDown",
  "version": "0.0.1",
  "icons": { "16": "icon_16.png",
             "48": "icon_48.png",
            "128": "icon_128.png" },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Sample Popup",    // optional; shown in tooltip
    "default_popup": "popup.html"
  },
    "background": {"scripts": ["background.js"]},

  "commands": {
  "_execute_browser_action": {
        "suggested_key": {
          "windows": "Ctrl+PageUp",
          "mac": "MacCtrl+PageUp"
        }
      },

   "test": {
         "suggested_key": {
           "default": "Ctrl+PageDown",
           "mac": "MacCtrl+PageDown"
         },
         "description": "Test alert function"
       }

  }
}
